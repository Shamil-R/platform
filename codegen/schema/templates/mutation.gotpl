{{- define "mutation" }}
type Mutation {
{{- range $i, $def := . }}
    create{{ .Name }}(data: {{ .Name }}CreateInput!): {{ .Name }}
    update{{ .Name }}(data: {{ .Name }}UpdateInput!, where: {{ .Name }}WhereUniqueInput!): {{ .Name }}
    delete{{ .Name }}(where: {{ .Name }}WhereUniqueInput!): {{ .Name }}
{{- end }}
}
{{- end }}
