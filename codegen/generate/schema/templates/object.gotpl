{{- define "object" }}
{{- range . }}
type {{ .Name }}
	{{- template "directives" . }} {
	{{- range .Fields.NotRelations }}
	{{ template "field" . }}
	{{- template "directives" . }}
	{{- end }}
	{{- range .Fields.RelationsOneToMany }}
	{{ .Name }}(where: {{ .Type.Name }}WhereInput): {{ .Type }}
	{{- template "directives" . }}
	{{- end }}
	{{- range .Fields.RelationsManyToOne }}
	{{ template "field" . }}
	{{- template "directives" . }}
	{{- end }}
}
{{ end }}
{{- end }}