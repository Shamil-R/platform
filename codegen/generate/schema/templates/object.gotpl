{{- define "object" }}
{{- range . }}
type {{ .Name }}{{ template "directives" .Directives }} {
    {{- range .Fields.NotRelations }}
	{{ .Name }}: {{ .Type }}
	{{- template "directives" .Directives }}
	{{- end }}
	{{- range .Fields.Relations }}
	{{ .Name }}(where: {{ .Type.Name }}WhereInput): {{ .Type }}
	{{- template "directives" .Directives }}
	{{- end }}
}
{{ end }}
{{- end }}