{{- define "query" }}
type Query {
{{- range . }}
    {{ untitle .Name }}(where: {{ .Name }}WhereUniqueInput!): {{ .Name }}
    {{ untitle .Name | plural }}(where: {{ .Name }}WhereInput, orderBy: {{ .Name }}OrderByInput, skip: Int, first: Int, last: Int, withTrashed: Boolean, onlyTrashed: Boolean): [{{ .Name }}]!
{{- end }}
}
{{- end }}