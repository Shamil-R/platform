{{- define "collection" }}
	tx, err := mssql.Begin(ctx)
	if err != nil {
		return nil, err
	}

	querySelect := `
		SELECT *
		FROM {{ template "table" . }}
	`

	argSelect := map[string]interface{}{}

	stmt, err := tx.PrepareNamed(querySelect)
	if err != nil {
		return nil, err
	}

	var resSelect {{ template "type" .Type }}

	if err := stmt.Select(&resSelect, argSelect); err != nil {
		return nil, err
	}

	return resSelect, nil
{{- end }}