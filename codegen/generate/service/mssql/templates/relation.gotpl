{{- define "relation" }}
	res := {{ template "new" .Type }}

	{{- $funcName := "RelationCollection" }}
	{{- if eq .Directives.Relation.ArgType "many_to_one" }}
		{{- $funcName := "RelationItem" }}
	{{- end }}

	if err := mssql.{{$funcName}}(ctx, obj.ID, &res); err != nil {
		return res, err
	}


	return res, nil
{{- end }}