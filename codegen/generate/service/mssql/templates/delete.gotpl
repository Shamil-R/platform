	{{- $table := lower .Name }}
	tx, err := mssql.Begin(ctx)
	if err != nil {
		return nil, err
	}

	r, err := {{ lower .Name }}(ctx, where.ID)
	if err != nil {
		return nil, err
	}

	query := `
		DELETE FROM [{{ $table }}]
		WHERE id = :id
	`

	arg := map[string]interface{}{
		"id": where.ID,
	}

	if _, err = tx.NamedExec(query, arg); err != nil {
		return nil, err
	}

	return r, nil